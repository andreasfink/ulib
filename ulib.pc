prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Cflags: -I${includedir}  -DHAVE_OPENSSL=1
Libs: -L${libdir} -l:libulib.so.2.0 -lobjc -lavahi-client -lavahi-core -lavahi-common -lgnustep-base -ldl -lsctp -lbsd -luuid

Name: ulib
Description: universal library
Version: 2.0.0
Requires: openssl 


