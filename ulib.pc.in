prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Cflags: -I${includedir}  -DHAVE_OPENSSL=1
Libs: -L${libdir} -lulib -lobjc -lavahi-client -lavahi-core -lavahi-common -lsctp -lgnustep-base -lssl -lcrypto -lbsd -luuid

Name: ulib
Description: universal library
Version: @VERSION@
Requires: uuid


